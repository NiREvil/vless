    },
    "dns": {
        "servers": [
            {
                "address": "https://208.67.222.222/dns-query",
                "address_resolver": "dns-local",
                "detour": "Proxy",
                "strategy": "ipv4_only",
                "tag": "dns-remote"
            },
            {
                "address": "tcp://8.8.4.4",
                "address_resolver": "dns-local",
                "detour": "direct",
                "strategy": "ipv4_only",
                "tag": "dns-direct"
            },
            {
                "address": "tcp://8.8.4.4",
                "detour": "direct",
                "tag": "dns-local"
            },
            {
                "address": "rcode://success",
                "tag": "dns-block"
            }
        ],
        "rules": [
            {
                "domain_suffix": [
                    ".ir"
                ],
                "rule_set": [
                    "geosite-ir"
                ],
                "server": "dns-direct"
            },
            {
                "disable_cache": true,
                "rule_set": [
                    "geosite-category-ads-all",
                    "geosite-malware",
                    "geosite-phishing",
                    "geosite-cryptominers"
                ],
                "server": "dns-block"
            },
            {
                "outbound": "any",
                "server": "dns-direct"
            }
        ],
        "independent_cache": true
    },
    "inbounds": [
        {
            "type": "tun",
            "tag": "tun-in",
            "domain_strategy": "ipv4_only",
            "interface_name": "sing-tun",
            "inet4_address": "172.19.0.1/30",
            "inet6_address": "fdfe:dcba:9876::1/126,
            "domain_strategy": "prefer_ipv4",
            "mtu": 9000,
            "auto_route": true,
            "strict_route": true,
            "stack": "system",
            "endpoint_independent_nat": true,
            "sniff": true,
            "sniff_override_destination": false
        }
    ],
    "outbounds": [
        {
            "tag": "Proxy",
            "type": "selector",
            "outbounds": [
                "URL-TEST",
                "KevinVPN"
            ],
            "interrupt_exist_connections": false
        },
        {
            "tag": "URL-TEST",
            "type": "urltest",
            "outbounds": [
                "KevinVPN"
            ],
            "url": "http://www.gstatic.com/generate_204",
            "interval": "10m",
            "tolerance": 50,
            "interrupt_exist_connections": false
        },
        {
            "tag": "direct",
            "type": "direct"
        },
        {
            "tag": "block",
            "type": "block"
        },
        {
            "tag": "dns-out",
            "type": "dns"
        },
        {
            "tag": "KevinVPN",
            "type": "hysteria2",
            "server": "kevin.iserver.store",
            "server_port": 8443,
            "up_mbps": 0,
            "down_mbps": 0,
            "password": "Telegram:@KevinZakarian",
